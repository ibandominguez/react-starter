{
  "name": "react-starter",
  "main": "src/app.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --progress --hide-modules --colors --hot --inline --history-api-fallback",
    "build": "NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest tests"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/file.js",
      "\\.(css|less)$": "<rootDir>/tests/__mocks__/style.js"
    }
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "jest": "^20.0.4",
    "on-build-webpack": "^0.1.0",
    "react-test-renderer": "^15.5.4",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "radium": "^0.19.4",
    "react": "^15.2.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
